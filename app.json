{
  "expo": {
    "name": "trade-up",
    "slug": "trade-up",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./core/view/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./core/view/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "package": "com.brennoamaral.tradeup"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./core/view/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4742f8a7-4799-4466-9abd-4295fd2f08e3"
      }
    }
  }
}
